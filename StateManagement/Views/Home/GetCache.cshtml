@{
    ViewData["Title"] = "GetCache";
}


@using Microsoft.Extensions.Caching.Memory;
@inject IMemoryCache cache


@{
    //Cache servisi viewdan çağırmak için viewcontext kullanmıyoruz. Direkt IMemoryCache servis üzerinden @inject ile view depedency injection yapıyoruz.
    var model = cache.Get<List<CacheModel>>("Ids") ?? new List<CacheModel>();
}

<h1>Get Cache</h1>
<div>
    @foreach (var item in model)
    {
        <div>@item.Id</div>
    }
</div>

